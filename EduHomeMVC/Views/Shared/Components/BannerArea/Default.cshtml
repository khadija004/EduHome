@model  Dictionary<string, string>

<!-- Banner Area Start -->
<div class="banner-area-wrapper">
    <div class="banner-area text-center" style='background: rgba(0, 0, 0, 0) url("/assets/img/banner/@Model["BannerAreaBackgroundImage"]") no-repeat scroll center center;'>
        <div class="container">
            <div class="row">
                <div class="col-xs-12">
                    <div class="banner-content-wrapper">
                        <div class="banner-content">
                            <h2>@ViewBag.Head</h2>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<!-- Banner Area End -->